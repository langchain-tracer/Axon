{
  "name": "agent-trace-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend for Agent Trace Visualizer with SQLite",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:init": "tsx src/database/schema.ts",
    "db:seed": "tsx src/database/seed.ts"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "openai": "^6.6.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
